{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/index.jsx",
    "app/(tabs)/_layout.jsc",
    "app/(tabs)/(distibutor)/distributorAdd.jsx",
    "app/(tabs)/Distributor/dashboard.jsx",
    "app/(tabs)/Distributor/products.jsx",
    "app/(tabs)/Distributor/profile.jsx",
    "app/_layout.jsx",
    "app/SplashScreenView.jsx",
    "app/+not-found.jsx",
    "app/(auth)/_layout.jsx",
    "app/(auth)/Onboarding1.jsx",
    "app/(auth)/Onboarding2.jsx",
    "app/(auth)/Onboarding3.jsx",
    "app/(auth)/SignUpDistributor.jsx",
    "app/(auth)/SignUpPharmacy.jsx",
    "app/(auth)/UserType.jsx"
  ]
}
